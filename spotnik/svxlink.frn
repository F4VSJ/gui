[GLOBAL]
LOGICS=SimplexLogic
CFG_DIR=svxlink.d
TIMESTAMP_FORMAT=%c
CARD_SAMPLE_RATE=48000

[SimplexLogic]
TYPE=Simplex
RX=Rx1
TX=Tx1
MODULES=ModuleHelp,ModuleMetarInfo,ModulePropagationMonitor,ModuleParrot,ModuleFrn
CALLSIGN=
SHORT_IDENT_INTERVAL=15
LONG_IDENT_INTERVAL=60
IDENT_ONLY_AFTER_TX=10
EXEC_CMD_ON_SQL_CLOSE=500
EVENT_HANDLER=/usr/share/svxlink/events.tcl
DEFAULT_LANG=fr_FR
RGR_SOUND_DELAY=0
REPORT_CTCSS=71.9
TX_CTCSS=ALWAYS
FX_GAIN_NORMAL=-6
FX_GAIN_LOW=-12
ACTIVATE_MODULE_ON_LONG_CMD=7:Frn
CTSS_FQ=undefined

[Rx1]
TYPE=Local
AUDIO_DEV=alsa:plughw:0
AUDIO_CHANNEL=1
SQL_DET=GPIO
SQL_START_DELAY=0
SQL_DELAY=45
SQL_HANGTIME=1800
SQL_EXTENDED_HANGTIME=1000
SQL_EXTENDED_HANGTIME_THRESH=15
SQL_TIMEOUT=600
VOX_FILTER_DEPTH=100
VOX_THRESH=1000
CTCSS_MODE=2
CTCSS_FQ=71.9
CTCSS_SNR_OFFSET=0
CTCSS_OPEN_THRESH=15
CTCSS_CLOSE_THRESH=9
CTCSS_BPF_LOW=60
CTCSS_BPF_HIGH=270
GPIO_PATH=/sys/class/gpio_sw
GPIO_SQL_PIN=PA10
DEEMPHASIS=0
SQL_TAIL_ELIM=0
PREAMP=-6
PEAK_METER=0
DTMF_DEC_TYPE=INTERNAL
DTMF_MUTING=1
DTMF_HANGTIME=40
1750_MUTING=1
CTSS_FQ=undefined

[Tx1]
TYPE=Local
AUDIO_DEV=alsa:plughw:0
AUDIO_CHANNEL=0
PTT_TYPE=GPIO
GPIO_PATH=/sys/class/gpio_sw
PTT_PIN=PA7
TIMEOUT=300
TX_DELAY=2
PREAMP=0
CTCSS_FQ=71.9
CTCSS_LEVEL=9
PREEMPHASIS=0
DTMF_TONE_LENGTH=100
DTMF_TONE_SPACING=50
DTMF_DIGIT_PWR=-15
CTSS_FQ=undefined

[LocationInfo]
APRS_SERVER_LIST=euro.aprs2.net:14580
LON_POSITION=00.00.00W
LAT_POSITION=00.00.00N
CALLSIGN=
FREQUENCY=434.975
TX_POWER=3
ANTENNA_GAIN=1
ANTENNA_HEIGHT=4m
ANTENNA_DIR=-1
PATH=WIDE1-1
BEACON_INTERVAL=10
TONE=71
COMMENT=>>> HotSpot-Spotnik V1.0 <<<<<
